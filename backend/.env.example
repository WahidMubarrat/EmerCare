# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
# For local development: mongodb://localhost:27017/emercare
# For MongoDB Atlas: mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>?retryWrites=true&w=majority
MONGODB_URI=mongodb://localhost:27017/emercare

# Cloudinary Configuration (for image/document uploads)
# Sign up at https://cloudinary.com to get your credentials
CLOUDINARY_CLOUD_NAME=your_cloud_name_here
CLOUDINARY_API_KEY=your_api_key_here
CLOUDINARY_API_SECRET=your_api_secret_here
